<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
"http://www.w3.org/TR/html4/loose.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    </head>
    <body>
        <table width=100%>
            <tr>
                <td>
                    <%= image_tag("logo_header_01.gif") %>
                    <hr width=100%>
                </td>
            </tr>
        </table>
        <table align=center>
<%= form_tag(:action => :input, :method => :get) do %>
            <tr>
                <td>
    <%= submit_tag "アンケート入力" %>
                </td>
            </tr>
<% end %>
<%= form_tag(:action => :input, :method => :get) do %>
            <tr>
                <td>
    <%= submit_tag "アンケート集計" %>
                </td>
            </tr>
<% end %>
<%= form_tag( :controller => :create_list, :method => :get) do %>
            <tr>
                <td>
    <%= submit_tag "アンケート編集" %>
                </td>
            </tr>
<% end %>
<%= form_tag(:action => :input, :method => :get) do %>
            <tr>
                <td>
    <%= submit_tag "logout" %>
                </td>
            </tr>
<% end %>
        </table>
    </body>
</html>